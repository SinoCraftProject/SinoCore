{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contributors": [
    "${mod_credits}"
  ],
  "icon": "assets/sinocore/logo.png",
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_sources}",
    "issues": "${mod_issues_page}"
  },
  "license": "${mod_license}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "games.moegirl.sinocraft.sinocore.fabric.SinoCoreFabric"
    ],
    "client": [
      "games.moegirl.sinocraft.sinocore.fabric.client.SinoCoreFabricClient"
    ]
  },
  "mixins": [
    "sinocore-common.mixins.json",
    "sinocore-fabric.mixins.json"
  ],
  "depends": {
    "java": "${fabric_java_version_range}",
    "minecraft": "${fabric_minecraft_version_range}",
    "fabricloader": "${fabric_loader_version_range}",
    "fabric": "${fabric_api_version_range}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/sinocore",
        "modmenu.modrinth": "https://modrinth.com/mod/sinocore",
        "modmenu.wiki": "https://docs.sino.moegirl.games/dev/",
        "modmenu.github_releases": "https://github.com/SinoCraftProject/SinoCore/releases",
        "modmenu.discord": "https://discord.gg/Q6zuDWDHJV"
      },
      "badges": ["library"],
      "update_checker": true
    }
  }
}